<h4 class="fg-white margin-vertical-medium sidebar-header">New Horizons</h4>
<div class="sidebar-sep"></div>
{{!--MAIN SECTION--}}
<div class="v-box" id="main-tabs">
    <button id="sidebar-button-home"
        class="sidebar-button-1 {{if (eq this.manager.router.currentRouteName 'main.home') 'sidebar-button-1-highlighted'}}"
        type="button" {{on 'click' (fn this.goToTab "home" true)}}>
        <FaIcon @icon="home" @size={{this.manager.constants.sidebarIconSize}}
            class="margin-sides-medium fg-teal-neon" />
        <p class="margin-vertical-auto">
            {{localize "Misc_Home"}}</p>
    </button>
    <div class="sidebar-sep"></div>
    <button id="sidebar-button-imprint"
        class="sidebar-button-1 {{if (eq this.manager.router.currentRouteName 'main.imprint') 'sidebar-button-1-highlighted'}}"
        type="button" {{on 'click' (fn this.goToTab "imprint" true)}}>
        <FaIcon @icon="info-circle" @size={{this.manager.constants.sidebarIconSize}}
            class="margin-sides-medium fg-teal-neon" />
        <p class="margin-vertical-auto">
            {{localize "Misc_Imprint"}}</p>
    </button>
    <div class="sidebar-sep"></div>
    <button id="sidebar-button-discord" class="sidebar-button-1" type="button" {{on 'click' this.goToDiscord}}>
        <FaIcon @icon="discord" @prefix="fab" @size={{this.manager.constants.sidebarIconSize}} class="margin-sides-medium
        fg-teal-neon" />
        <p class="margin-vertical-auto">
            Discord</p>
    </button>
</div>
{{!--EDITOR SECTION--}}
<div class="sidebar-sep"></div>
<h4 class="fg-white margin-vertical-medium sidebar-header">{{localize "Misc_Editor"}}</h4>
<div class="sidebar-sep"></div>
<button class="sidebar-button-1" type="button" {{on 'click' (fn
    this.manager.showStellarpediaEntry "basic-rules" "introduction" "welcome" )}}>
    <FaIcon @icon="sun" @size={{this.manager.constants.sidebarIconSize}} class="margin-sides-medium fg-teal-neon" />
    <p class="margin-vertical-auto">
        {{localize "Misc_Stellarpedia"}}</p>
</button>
<div class="sidebar-sep"></div>
<button class="sidebar-button-1" type="button" {{on 'click' (fn this.goToTab "generator.preset" )}}>
    <FaIcon @icon="user-plus" @size={{this.manager.constants.sidebarIconSize}}
        class="margin-sides-medium fg-teal-neon" />
    <p class="margin-vertical-auto">
        {{localize "Misc_NewCharacter"}}</p>
</button>
